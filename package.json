{
  "name": "api-agg-decorator",
  "version": "1.0.0",
  "description": "using decorator to automatically generate doc and controller map",
  "private": true,
  "main": "dist/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch:ts": "tsc -w -p tsconfig.json && node ./dist/test.js",
    "start": "nodemon --watch lib -e ts --exec ts-node ./lib/test.ts"
  },
  "keywords": [
    "decorator",
    "api-doc",
    "SDK"
  ],
  "author": "",
  "dependencies": {
    "is-type-of": "^1.2.1",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@types/koa": "^2.0.49",
    "@types/ramda": "^0.26.21",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
